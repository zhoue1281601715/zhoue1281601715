page {
  background: #e6e6e6;
}

.details {
  padding: 20rpx 18rpx 114rpx;
  width: 750rpx;
  box-sizing: border-box;
}

.orderName {
  background: #fff;
  border-radius: 10rpx;
}

.oederDetails {
  display: flex;
  justify-content: space-between;
  padding: 0 38rpx;
  height: 74rpx;
  line-height: 72rpx;
  font-size: 30rpx;
  color: #010101;
  border-bottom: 2rpx solid #e5e5e5;
}

.companyName {
  margin-top: 20rpx;
  background: #fff;
  border-radius: 10rpx;
}

.companyDetails {
  display: flex;
  justify-content: space-between;
  padding: 0 38rpx;
  height: 74rpx;
  line-height: 72rpx;
  font-size: 30rpx;
  color: #010101;
  border-bottom: 2rpx solid #e5e5e5;
}

.queryTitle {
  position: relative;
  padding-left: 36rpx;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  color: #000000;
  box-sizing: border-box;
}

.queryTitle:before {
  position: absolute;
  top: 20rpx;
  left: 16rpx;
  display: block;
  content: '';
  width: 0;
  height: 40rpx;
  border-left: 4rpx solid #FF5301;
}

.relation_num {
  color: #FF5301;
}

.goods-item {
  margin-bottom: 16rpx;
  border-radius: 10rpx;
  background-color: #FFFFFF;
}

.goods-item:last-child {
  margin: 0;
}

.goods-name {
  display: flex;
  align-items: center;
  padding: 12rpx 36rpx 0;
  height: 84rpx;
  font-size: 28rpx;
  color: #000000;
  box-sizing: border-box;
  border-bottom: 2rpx solid #E5E5E5;
}

.goods-data {
  display: flex;
  padding: 12rpx 12rpx 16rpx 32rpx;
  box-sizing: border-box;
}

.goods-data-left {
  width: 400rpx;
}

.goods-data-item {
  display: flex;
  align-items: center;
  height: 46rpx;
}

.goods-data-name, .goods-data-val {
  font-size: 24rpx;
  color: #000000;
}


.goods-data-right {
  flex: 1;
}