@import '/lib/iconfont.wxss';

.main-nav {
  position: fixed;
  left: 0;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  width: 100%;
  height: 78rpx;
  background: linear-gradient(90deg, #FF7900, #FF4B00);
  z-index: 9;
}

.main-nav-item {
  flex: 1;
  line-height: 62rpx;
  font-size: 28rpx;
  color: #FFFFFF;
  text-align: center;
}

.main-nav-activity {
  font-size: 36rpx;
}

.main-content {
  padding: 78rpx 0 330rpx;
}

.main-content-area {
  width: 750rpx;
  background-color: #FFFFFF;
}

.search-area {
  padding: 22rpx 24rpx;
  width: 750rpx;
  background-color: #FFFFFF;
  box-sizing: border-box;
}

.search {
  display: flex;
  align-items: center;
  padding: 0 14rpx;
  width: 100%;
  height: 52rpx;
  background-color: #EEEEEE;
  border-radius: 26rpx;
  box-sizing: border-box;
}

.search-icon {
  width: 28rpx;
}

.search-input {
  flex: 1;
  padding: 0 10rpx;
  height: 52rpx;
  font-size: 20rpx;
  color: #000000;
}

.input-placeholder {
  color: #838383;
}

.goods-list {
  flex: 1;
  padding: 0 32rpx;
}

.goods-item {
  display: flex;
  justify-content: flex-start;
  padding: 24rpx 0;
  border-bottom: 2rpx solid #EDEDED;
}

.goods-item:first-child {
  padding-top: 0;
}

.goods-picture-area {
  width: 190rpx;
  height: 160rpx;
  border-radius: 10rpx;
  overflow: hidden;
}

.goods-picture {
  width: 190rpx;
  height: 160rpx;
}

.goods-data {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: stretch;
  flex: 1;
  margin-left: 20rpx;
}

.goods-name {
  font-size: 28rpx;
  color: #000000;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.goods-number {
  font-size: 20rpx;
  color: #5B5B5B;
}

.goods-data-bottom {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}

.goods-specs {
  font-size: 24rpx;
  color: #FF4B00;
}

.goods-specs-title {
  font-size: 18rpx;
}

.apply-btn {
  margin: 0;
  padding: 0 16rpx;
  height: 56rpx;
  line-height: 52rpx;
  font-size: 24rpx;
  color: #FF4B00;
  border: 2rpx solid #FF4B00;
  border-radius: 28rpx;
  background-color: #FFFFFF;
  box-sizing: border-box;
}

.out-of-warehouse {
  padding: 24rpx 18rpx;
  width: 750rpx;
  box-sizing: border-box;
}

.form-area {
  width: 100%;
  border-radius: 6rpx;
  background-color: #FFFFFF;
}

.form-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 14rpx 0 38rpx;
  height: 82rpx;
  box-sizing: border-box;
}

.form-border {
  border-bottom: 2rpx solid #E5E5E5;
}

.form-item-left {
  font-size: 28rpx;
  color: #010101;
}

.form-item-right {
  display: flex;
  align-items: center;
}

.form-item-right .picker {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 24rpx;
  color: #010101;
}

.form-item-right .picker-default {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 24rpx;
  color: #818181;
}

.product-name {
  padding-right: 56rpx;
  height: 80rpx;
  font-size: 24rpx;
  color: #010101;
  text-align: right;
}

.more-icon {
  margin-left: 28rpx;
  height: 28rpx;
}

.form-btn-area {
  padding: 20rpx 36rpx 30rpx;
  width: 100%;
  box-sizing: border-box;
}

.form-btn {
  margin: 0;
  padding: 0;
  width: 100%;
  height: 84rpx;
  line-height: 84rpx;
  font-size: 34rpx;
  color: #FFFFFF;
  text-align: center;
  background: linear-gradient(90deg, #FF7900, #FF4B00);
  border-radius: 42rpx;
}

.receipt-list {
  margin-top: 20rpx;
}

.receipt-item {
  margin-top: 20rpx;
  background-color: #FFFFFF;
  border-radius: 6rpx;
}

.receipt-item:first-child {
  margin: 0;
}

.receipt-name {
  padding-left: 36rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  color: #000000;
  box-sizing: border-box;
}

.receipt-data {
  padding: 16rpx 32rpx;
  border-top: 2rpx solid #E5E5E5;
  border-bottom: 2rpx solid #E5E5E5;
  box-sizing: border-box;
}

.receipt-data-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 46rpx;
}

.receipt-data-item-left {
  flex: 7;
  font-size: 24rpx;
  color: #000000;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.receipt-data-item-right {
  flex: 5;
  font-size: 24rpx;
  color: #000000;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.delivery-btn-area {
  display: flex;
  justify-content: flex-end;
  padding: 20rpx 36rpx;
  box-sizing: border-box;
}

.delivery-view {
  padding: 0 32rpx;
  height: 48rpx;
  line-height: 44rpx;
  font-size: 24rpx;
  color: #FF5402;
  border: 2rpx solid #FF5402;
  border-radius: 24rpx;
}

.warehousing-popup, .delivery-popup {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 99999;
}

.warehousing-bg, .delivery-bg {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, .65);
  box-sizing: border-box;
}

.warehousing-area, .delivery-area {
  padding: 20rpx 46rpx 40rpx;
  width: 626rpx;
  background-color: #FFFFFF;
  border-radius: 15rpx;
}

.warehousing-title, .delivery-title {
  height: 72rpx;
  line-height: 72rpx;
  font-size: 34rpx;
  text-align: center;
  color: #000000;
}

.warehousing-data, .delivery-data {
  padding-top: 36rpx;
}

.warehousing-item, .delivery-item {
  margin-top: 36rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 2rpx solid #E5E5E5;
}

.warehousing-item:first-child, .delivery-item:first-child {
  margin: 0;
}

.warehousing-item-left, .delivery-item-left {
  font-size: 30rpx;
  color: #000000;
  height: 38rpx;
  line-height: 38rpx;
}

.warehousing-item-right, .delivery-item-right {
  flex: 1;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.warehousing-input, .delivery-input, .delivery-val {
  flex: 1;
  padding: 0 20rpx;
  height: 38rpx;
  line-height: 38rpx;
  font-size: 30rpx;
  text-align: right;
  color: #000000;
  box-sizing: border-box;
}

.warehousing-unit, .delivery-unit {
  height: 38rpx;
  line-height: 38rpx;
  font-size: 30rpx;
  color: #7A7A7A;
}

.picker {
  height: 38rpx;
  line-height: 38rpx;
  font-size: 30rpx;
  color: #000000;
}

.picker-default {
  height: 38rpx;
  line-height: 38rpx;
  font-size: 30rpx;
  color: #7A7A7A;
}

.picker-right-arrow {
  margin-left: 8rpx;
  height: 28rpx;
}

.remarks {
  border: none;
}

.textarea-area {
  margin-top: 8rpx;
  padding: 10rpx;
  padding-bottom: 0;
  width: 100%;
  border: 2rpx solid #C9C9C9;
  box-sizing: border-box;
}

.textarea-area textarea {
  width: 100%;
  height: 160rpx;
  line-height: 40rpx;
}

.textarea-val-len {
  height: 42rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.not-full {
  height: 42rpx;
  line-height: 42rpx;
  font-size: 24rpx;
  color: #7A7A7A;
}

.full {
  height: 42rpx;
  line-height: 42rpx;
  font-size: 24rpx;
  color: #000000;
}

.warehousing-btn, .delivery-btn {
  margin: 52rpx auto 0;
  padding: 0 24rpx;
  width: 244rpx;
  height: 78rpx;
  line-height: 78rpx;
  font-size: 34rpx;
  text-align: center;
  color: #FFFFFF;
  background: linear-gradient(90deg, #FF7900, #FF4B00);
  border-radius: 36rpx;
}

.close-popup {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 32rpx;
  width: 58rpx;
  height: 58rpx;
  border: 2rpx solid #FFFFFF;
  border-radius: 29rpx;
  box-sizing: border-box;
}

.close-popup-icon {
  width: 32rpx;
}

.btn-area {
  position: fixed;
  bottom: 94rpx;
  left: 0;
  display: flex;
  justify-content: space-between;
  align-items: stretch;
  padding: 0 24rpx 0 48rpx;
  width: 750rpx;
  height: 130rpx;
  background: linear-gradient(90deg, #FF7900, #FF4B00);
  box-sizing: border-box;
}

.btn-area-left {
  display: flex;
  align-items: center;
}

.checkbox {
  position: relative;
  margin-right: 16rpx;
  width: 38rpx;
  height: 38rpx;
  border-radius: 50%;
  border: 2rpx solid #FFFFFF;
  background-color: #FF7400;
}

.checkbox::before {
  display: block;
  content: "\e645";
  padding-left: 4rpx;
  width: 38rpx;
  height: 38rpx;
  line-height: 38rpx;
  font-size: 28rpx;
  color: #FF7400;
  font-family: "cuIcon";
  pointer-events: none;
  box-sizing: border-box;
}

.checked {
  background-color: #FFFFFF;
}

.btn-area-left view {
  font-size: 24rpx;
  color: #444444;
}

.checked-num {
  margin-left: 16rpx;
}

.btn-area-right {
  position: relative;
  display: flex;
  align-items: center;
}

.btn-bg {
  position: absolute;
  top: -104rpx;
  right: 0;
  width: 256rpx;
}

.buy-btn {
  position: relative;
  width: 256rpx;
  line-height: 86rpx;
  font-size: 34rpx;
  color: #FF5002;
  text-align: center;
  z-index: 9;
}
/* 弹框 */
