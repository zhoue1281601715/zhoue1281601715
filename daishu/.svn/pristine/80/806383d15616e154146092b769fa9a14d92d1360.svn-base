/* packageC/data_statistics/data_statistics.wxss */
page{background: #f8f8fa;height: auto;}
.data{margin-left: 30rpx;margin-right: 30rpx;margin-top:20rpx;}
.data .statistics{background: #fff;border-radius: 10rpx;padding:25rpx 20rpx 10rpx 20rpx;height: calc(200px + 40rpx);overflow: hidden;}
.data .statistics .tit{color: #222222;font-size: 30rpx;font-weight: bold;line-height: 40rpx;}
.container{width: 100%;height:200px;}
ec-canvas{
  width: 100%;height:150px;
}
.timeList{margin: 0px 30rpx 20rpx 30rpx;}
.timeList .item{background: #fff;padding: 30rpx 0px;margin-top: 20rpx;display: flex;height: 120rpx;overflow: hidden;}
.timeList .item .ls{display: block;width: 33.333%;border-left: 1px solid #F7F7F7;}
.timeList .item .ls:first-child{border: 0;}
.timeList .item .ls .tits{padding-left: 50rpx;font-size: 20rpx;color: #999999;}
.timeList .item .ls .desc{font-size: 18rpx;color: #333333;}
.timeList .item .ls .desc text{display: inline-block;padding-left: 50rpx;font-size: 30rpx;font-weight: bold;}
.timeList .item .ls navigator{display: block;width: 100%;line-height: 60rpx;text-align: center;color: #FF8F32;font-size: 24rpx;}
.timeList .item .ls navigator image{width: 10rpx;vertical-align: middle;display: inline-block;margin-left: 11rpx;}
.c1{color: #666666;}
.c2{color: #333333;}