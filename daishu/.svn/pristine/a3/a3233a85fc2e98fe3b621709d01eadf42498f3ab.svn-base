.main {
  padding: 22rpx 22rpx 224rpx 22rpx;
  width: 750rpx;
  box-sizing: border-box;
}

.shopping-car-list {
  width: 100%;
  background-color: #FFFFFF;
  border-radius: 10rpx;
}

.shopping-car-edit-area {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  padding: 12rpx 0;
  border-bottom: 2rpx solid #DCDCDC;
}

.shopping-car-edit {
  padding: 0 40rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  color: #000000;
  border-left: 2rpx solid #DCDCDC;
}

.shopping-car-item {
  display: flex;
  justify-content: space-between;
  padding: 22rpx;
  width: 100%;
  box-sizing: border-box;
  border-bottom: 2rpx solid #DCDCDC;
}

.shopping-car-item:last-child {
  border: none;
}

.shopping-car-checkbox {
  position: relative;
  margin-top: 56rpx;
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  border: 2rpx solid #DCDCDC;
  background-color: #FFFFFF;
}

.shopping-car-checkbox::before {
  display: block;
  content: "\e645";
  padding-left: 2rpx;
  width: 38rpx;
  height: 38rpx;
  line-height: 30rpx;
  font-size: 24rpx;
  color: #FFFFFF;
  font-family: "cuIcon";
  pointer-events: none;
  box-sizing: border-box;
}

.shopping-car-checked {
  border-color: #FF7400;
  background-color: #FF7400;
}

.shopping-car-item .goods-picture-area {
  margin-left: 24rpx;
  width: 180rpx;
  height: 180rpx;
  border-radius: 10rpx;
  overflow: hidden;
  background-color: #E5E5E5;
}

.shopping-car-item .goods-picture {
  width: 180rpx;
  height: 180rpx;
}

.shopping-car-item .goods-data {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-start;
  margin-left: 22rpx;
  width: 0;
  height: 180rpx;
}

.shopping-car-item .goods-name {
  width: 100%;
  font-size: 30rpx;
  line-height: 30rpx;
  color: #000000;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.shopping-car-item .set-meal-type {
  padding: 0 36rpx;
  line-height: 58rpx;
  font-size: 30rpx;
  color: #8F8F8F;
  background-color: #F9F9F9;
  border-radius: 10rpx;
}

.shopping-car-item .goods-price-area {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  width: 100%;
}

.shopping-car-item .goods-price {
  font-size: 38rpx;
  color: #FF5201;
}

.shopping-car-item .yuan {
  font-size: 32rpx;
}

.shopping-car-item .goods-num {
  padding: 0 12rpx;
  line-height: 52rpx;
  font-size: 30rpx;
  color: #000000;
  border: 2rpx solid #FF5201;
  border-radius: 10rpx;
  box-sizing: border-box;
}

.shopping-car-item .goods-num-area {
  display: flex;
  align-items: center;
}

.shopping-car-item .operator {
  width: 52rpx;
  line-height: 48rpx;
  font-size: 30rpx;
  text-align: center;
  color: #000000;
  border: 2rpx solid #FF5201;
  border-radius: 10rpx;
  box-sizing: border-box;
}

.shopping-car-item .operator-disabled {
  color: #E5E5E5;
  border-color: #E5E5E5;
}

.shopping-car-item .goods-num-input-area {
  margin: 0 6rpx;
  padding: 0 6rpx;
  height: 52rpx;
  border: 2rpx solid #FF5201;
  border-radius: 10rpx;
  box-sizing: border-box;
}

.shopping-car-item .goods-num-input {
  width: 52rpx;
  height: 48rpx;
  font-size: 30rpx;
  text-align: center;
  color: #000000;
}

.division-area {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 30rpx;
  height: 82rpx;
}

.division-line {
  width: 75rpx;
}

.division-tip {
  font-size: 30rpx;
  color: #FF5201;
  margin: 0 8rpx;
}

.goods-list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.goods-item {
  width: 348rpx;
  margin-bottom: 14rpx;
  padding-bottom: 32rpx;
  background-color: #FFFFFF;
  border-radius: 10rpx;
  overflow: hidden;
}

.goods-picture {
  width: 348rpx;
  vertical-align: bottom;
  background-color: #E5E5E5;
}

.goods-data {
  margin-top: 10rpx;
  padding: 0 18rpx;
  box-sizing: border-box;
}

.goods-label {
  display: inline-block;
  margin-right: 10rpx;
  padding: 0 8rpx;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 24rpx;
  color: #FFFFFF;
  background-color: #FE6706;
  border-radius: 6rpx;
}

.goods-name {
  font-size: 28rpx;
  color: #010101;
}

.goods-price-area {
  display: flex;
  align-items: baseline;
  margin-top: 14rpx;
}

.yuan {
  font-size: 20rpx;
  color: #FF6400;
}

.goods-price {
  font-size: 34rpx;
  color: #FF6400;
}

.payment-num {
  margin-left: 10rpx;
  font-size: 20rpx;
  color: #909090;
}

.more {
  display: flex;
  justify-content: flex-end;
  font-size: 24rpx;
  color: #909090;
}

.btn-area {
  position: fixed;
  bottom: 94rpx;
  left: 0;
  display: flex;
  justify-content: space-between;
  align-items: stretch;
  padding: 0 24rpx;
  width: 750rpx;
  height: 130rpx;
  background: linear-gradient(90deg, #FF7900, #FF4B00);
  box-sizing: border-box;
}

.btn-area-left {
  display: flex;
  align-items: center;
}

.checkbox {
  position: relative;
  width: 38rpx;
  height: 38rpx;
  border-radius: 50%;
  border: 2rpx solid #FFFFFF;
  background-color: #FF7400;
}

.checkbox::before {
  display: block;
  content: "\e645";
  padding-left: 4rpx;
  width: 38rpx;
  height: 38rpx;
  line-height: 38rpx;
  font-size: 28rpx;
  color: #FF7400;
  font-family: "cuIcon";
  pointer-events: none;
  box-sizing: border-box;
}

.checked {
  background-color: #FFFFFF;
}

.checked-num {
  font-size: 24rpx;
  color: #FFFFFF;
  margin-left: 16rpx;
}

.total-area {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
  color: #FFFFFF;
}

.total-val {
  font-size: 42rpx;
}

.btn-area-right, .delete-btn-area  {
  display: flex;
}

.btn-content {
  position: relative;
  display: flex;
  align-items: center;
  margin-left: 20rpx;
}

.btn-bg {
  position: absolute;
  top: -104rpx;
  right: 0;
  width: 256rpx;
}

.buy-btn, .delete-btn {
  position: relative;
  width: 256rpx;
  line-height: 86rpx;
  font-size: 34rpx;
  color: #FF5002;
  text-align: center;
  z-index: 9;
}

.nodata {
  padding: 50rpx 0;
  text-align: center;
  font-size: 28rpx;
  color: #818181;
}