page {
  padding-bottom: 170rpx;
}

.warehouseDetails {
  width: 696rpx;
  margin: 28rpx auto;
  position: relative;
}

.wareText {
  width: 100%;
  height: 372rpx;
  padding: 24rpx 18rpx 18rpx 24rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  position: absolute;
  top: 0;
  left: 0;
}

.ticket {
  display: flex;
  justify-content: space-between;
}

.tickets {
  font-size: 38rpx;
  font-family: AlibabaPuHuiTiM;
  font-weight: bold;
  color: #010101;
}

.purchase {
  padding: 0 24rpx;
  height: 28rpx;
  line-height: 24rpx;
  font-size: 16rpx;
  font-family: AlibabaPuHuiTiR;
  font-weight: normal;
  color: #FFFFFF;
  text-align: center;
  border: 1px solid #FFFFFF;
  border-radius: 14rpx;
}

.useTime {
  color: #fff;
  margin-top: 10rpx;
  font-size: 26rpx;
}

.wareTime {
  font-size: 16rpx;
}

.money {
  font-size: 76rpx;
  font-family: AlibabaPuHuiTiB;
  font-weight: bold;
  color: #FFFFFF;
}

.money text {
  font-size: 38rpx;
}

.surplus {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 750rpx;
  height: 58rpx;
  background: #C9C9C9;
  padding: 0 50rpx 0 44rpx;
  box-sizing: border-box;
}

.progress-area {
  display: flex;
  align-items: center;
}

.progress {
  width: 168rpx;
  height: 12rpx;
  border-radius: 6rpx;
  background-color: #FFFFFF;
  overflow: hidden;
}

.progress-ratio {
  height: 12rpx;
  background-color: #FF4B00;
}

.progress-value {
  margin-left: 16rpx;
  font-size: 26rpx;
  color: #000000;
}

.surplus-value {
  font-size: 26rpx;
  color: #000000;
}

.volume {
  font-size: 38rpx;
  font-family: AlibabaPuHuiTiM;
  font-weight: bold;
  color: #010101;
}

.wareContent {
  padding: 48rpx 42rpx;
}

.volumeMoney {
  font-size: 54rpx;
  font-family: AlibabaPuHuiTiB;
  font-weight: bold;
  color: #FF5301;
}

.volumeMoney text {
  font-size: 28rpx;
  font-family: AlibabaPuHuiTiB;
  font-weight: bold;
  color: #FF5301;
}

.volumeContent {
  display: flex;
  align-items: baseline;
}

.careful {
  margin-left: 24rpx;
  font-size: 18rpx;
  font-family: AlibabaPuHuiTiR;
  font-weight: normal;
  color: #909090;
}

.explain {
  font-family: AlibabaPuHuiTiR;
  margin-top: 50rpx;
}

.explainTitle {
  font-size: 34rpx;
  font-family: AlibabaPuHuiTiM;
  font-weight: bold;
  color: #000000;
}

.explainContent {
  margin-top: 14rpx;
  line-height: 24rpx;
  font-size: 18rpx;
  font-weight: normal;
  color: #9F9F9F;
}

.rushPurchase {
  margin: 24rpx auto 0;
  padding: 0;
  width: 244rpx;
  height: 78rpx;
  line-height: 78rpx;
  background-image: linear-gradient(to right, #FF7900, #FF4B00);
  font-size: 38rpx;
  color: #fff;
  border-radius: 39rpx;
}

.popup-area {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: rgba(0, 0, 0, .35);
}

.popup-content {
  display: flex;
  align-items: flex-end;
  width: 100%;
  height: 100%;
}

.popup {
  position: relative;
  left: 0;
  transition: bottom 300ms linear;
  padding: 36rpx 20rpx 132rpx;
  width: 100%;
  background-color: #FFFFFF;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  box-sizing: border-box;
}

.close-popup {
  display: flex;
}

.close-icon {
  width: 22rpx;
}

.payment-amount {
  display: flex;
  justify-content: center;
  align-items: baseline;
  margin-top: 18rpx;
  font-size: 54rpx;
  color: #000000;
  font-family: AlibabaPuHuiTiB;
  font-weight: bold;
}

.payment-amount .yuan {
  font-size: 28rpx;
}

.payment-type {
  display: flex;
  /* justify-content: space-around; */
  justify-content: center;
  align-items: center;
  margin-top: 68rpx;
  padding-bottom: 44rpx;
}

.payment-type-item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.payment-type-icon {
  width: 142rpx;
}

.payment-name {
  margin-top: 18rpx;
  font-size: 28rpx;
  color: #010101;
}