/* pages/in_stock_res/in_stock_res.wxss */
page {
    background-color: rgb(247, 247, 248);
}

.res_wrap {
    width: 100%;
    height: 500rpx;
    padding-top: 103rpx;
    background-color: #ffffff;
    margin-bottom: 60rpx;
    text-align: center;
}

.res_wrap image {
    display: block;
    width: 190rpx;
    height: 175rpx;
    margin: 0 auto;
}

.res_wrap .res_text {
    font-size: 36rpx;
    font-family: PingFang-SC-Bold;
    font-weight: bold;
    color: #343434;
    margin-bottom: 30rpx;
    display: block;
}

.res_wrap .res_tips {
    font-size: 28rpx;
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    color: #9A9A9A;
}

.btn {
    width: calc(100% - 60rpx);
    margin: 0rpx auto 30rpx auto;
    height: 88rpx;
    text-align: center;
    line-height: 88rpx;
    border-radius: 44rpx;
}

.first_btn {
    background: linear-gradient(90deg, #FF7900, #FF4B00);
    color: #ffffff;
    font-size: 34rpx;
    font-family: PingFang-SC-Bold;
    font-weight: bold;
}

.back_index {
    border: 1rpx solid #9A9A9A;
    font-size: 34rpx;
    font-family: PingFang-SC-Medium;
    font-weight: Medium;
    color: #9A9A9A;
}