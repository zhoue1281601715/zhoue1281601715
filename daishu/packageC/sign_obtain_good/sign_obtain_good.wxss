/* packageC/sign_obtain_good/sign_obtain_good.wxss */
page {
    background: #f8f8fa;
    padding-bottom: 20rpx;
}

.t_Tab {
    border-top: 1px solid #F7F7F7;
    height: 90rpx;
    display: flex;
    text-align: center;
    background: #fff;
    line-height: 90rpx;
}

.t_Tab .item {
    width: 50%;
    position: relative;
    font-size: 30rpx;
    color: #666666;
    height: 100%;
}

.item {
    padding-bottom: 20rpx;
}


.t_Tab .item.act {
    color: #FF8F32;
    font-weight: bold;
}

.t_Tab .item.act::after {
    position: absolute;
    content: " ";
    width: 60rpx;
    height: 4rpx;
    bottom: 0;
    left: calc(50% - 30rpx);
    display: block;
    background: #FF8F32;
}

.rewu .item {
    margin: 30rpx 20rpx 0 20rpx;
    background: #fff;
    border-radius: 10rpx;
    display: block;
}

.rewu .item .tit {
    height: 90rpx;
    line-height: 90rpx;
    padding: 0 31rpx;
    border-bottom: 1px solid #F2F2F2;
}

.rewu .item .tit .name {
    display: inline-block;
    font-size: 30rpx;
    color: #333333;
    font-weight: bold;
}

.rewu .item .tit .type {
    float: right;
    font-size: 24rpx;
    color: #FF8F32;
    line-height: 48rpx;
    padding: 0 16rpx;
    border-radius: 4rpx;
    background: rgba(255, 143, 50, 0.1);
    margin-top: 20rpx;
}

.rewu .item .n {
    padding: 15px 0;
    margin: 0px 30rpx;
    line-height: 58rpx;
}

.rewu .item .n .d1 {
    display: inline-block;
    width: 50%;
    color: #333;
    font-size: 28rpx;
}

.rewu .item .n .d2 {
    display: inline-block;
    width: 100%;
    color: #333;
    font-size: 28rpx;
}

.rewu .item .n .d1 text,
.rewu .item .n .d2 text {
    display: inline-block;
    margin-right: 20rpx;
    font-weight: bold;
}

.rewu .item .n .d1 text:last-child,
.rewu .item .n .d2 text:last-child {
    margin-right: 0rpx;
}

.rewu .item .n .desc {
    background: #F8F8FA;
    padding: 21rpx;
    color: #333333;
    font-size: 28rpx;
    line-height: 34rpx;
    margin-top: 15rpx;
}

.rewu .item .n .site {
    margin-left: 66rpx;
    position: relative;
    /* height: 230rpx; */
}

.rewu .item .n .site:nth-of-type(2) {
    padding-top: 20rpx;
}

.rewu .item .n .site:nth-of-type(2) .img {
    top: 30rpx;
}

.rewu .item .n .site:last-child .r {
    border: none;
}

.rewu .item .n .site .img {
    width: 32rpx;
    left: -66rpx;
    top: 10rpx;
    position: absolute;
}

.rewu .item .n .site .r {
    border-bottom: 1px solid #F2F2F2;
}

.rewu .item .n .site .t {
    line-height: 50rpx;
}

.rewu .item .n .site .t .name {
    display: inline-block;
    font-size: 28rpx;
    font-weight: bold;
    color: #333333;
    margin-right: 32rpx;
}

.rewu .item .n .site .t .phone {
    display: inline-block;
    font-size: 28rpx;
    color: #333333;
}

.rewu .item .n .site .descs {
    line-height: 46rpx;
    color: #333;
    padding: 7rpx 0;
}

.rewu .item .n .site .bPhone {
    width: 148rpx;
    margin: 30rpx 0;
    text-align: center;
    line-height: 54rpx;
    font-size: 20rpx;
    color: #333;
    border-radius: 27rpx;
    border: 1px solid #DDDDDD;
}

.rewu .item .n .site .bPhone image {
    width: 20rpx;
    height: 24rpx;
    display: inline-block;
    margin-right: 10rpx;
    vertical-align: middle;
}

/* 弹窗 */
van-popup {
    padding: 20rpx !important;
    box-sizing: border-box !important;
}

van-icon {
    display: none !important;

}
.van-popup--bottom.van-popup--safe{
    padding: 20rpx !important;
    box-sizing: border-box !important;
}

.van-cell .van-cell__value {
    text-align: center !important;
    font-size: 38rpx !important;
    color: #333333 !important;
}

