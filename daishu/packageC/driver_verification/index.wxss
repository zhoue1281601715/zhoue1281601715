.main {
  padding: 0 16rpx 122rpx;
  width: 750rpx;
 
}
.user-info {
  border-radius: 6rpx;
  background-color: #FFFFFF;
  box-shadow: 0 6rpx 10rpx 0 rgba(153, 153, 153, 0.35);
  border-bottom: 1px solid #fff;
  padding-bottom: 65rpx;
}
.user-avatar {
  display: flex;
  justify-content: center;
  
}

.avatar {
  margin-top: 20rpx;
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  border: 1px solid #FF7900;
  box-shadow: 1rpx 1rpx 9rpx #aca8a8;
}
.login-tip {
  margin-top: 20rpx;
  font-size: 28rpx;
  /* color: #C3C3C3; */
  text-align: center;
  color: #aca8a8;
  font-weight: 600;
}
.logout-area {
  padding-top: 131rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.logout {
  margin-left: 32%;
  margin-top: 14%;
  padding: 0 30rpx;
  width: 198rpx;
  height: 57rpx;
  line-height: 57rpx;
  font-size: 24rpx;
  color: #FFFFFF;
  text-align: center;
  border-radius: 6rpx;
  background-color: #FF7900;
}
.delivery-item {
  display: flex;
  margin-top: 29rpx;

  align-items: center;

  height: 50rpx;
  box-sizing: border-box;
}
.delivery-item-left {
  width: 180rpx;
  font-size: 28rpx;
  color: #000000;
  height: 50rpx;
  line-height: 50rpx;
  text-align: right;

}

.delivery-item-right {
  flex: 0.8;
  display: flex;
  margin-left: 16rpx;
  justify-content: flex-end;
  align-items: center;
}
.delivery-input {
  flex: 1;
  /* padding: 0 38rpx 0 20rpx; */
  height: 50rpx;
  font-size: 28rpx;
  color: #888888;
  box-sizing: border-box;
  border-bottom: 1rpx solid #888888;
}


.btn{
  background-color: #016db5;
}
.picture-area {
  width: 100%;
  height: 430rpx;

  border: 2rpx dashed #FF7900;
}

.picture-item {
  position: relative;
  width: 100%;
}

.remove {
  position: absolute;
  top: -2rpx;
  right: 53rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 50rpx;
  height: 50rpx;
  background-color: rgba(0, 0, 0, .35);
  border-radius: 25rpx;
  z-index: 9;
}

.remove-icon {
  width: 26rpx;
}

.imgs {
  width: 100%;
  height:200rpx;

}

.upload-picture {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 347rpx;
  font-size: 24rpx;
  color: #696969;
}
.carImgHei{
  height: 228rpx;
}
.section__ctn{
  text-align: center;
}
.clear{
  clear:both;
}
.tietlName {
  height: 83rpx;
  padding-left: 14rpx;
}
.marginTop {
  margin-top: 10px;
}