.nav {
  position: fixed;
  top: 0;
  left: 0;
  padding: 0 190rpx;
  width: 100%;
  height: 96rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #F2F2F2;
  box-sizing: border-box;
  z-index: 999;
}

.default {
  height: 82rpx;
  line-height: 82rpx;
  font-size: 30rpx;
  color: #1E1E1E;
  border-bottom: 6rpx solid #F2F2F2;
  box-sizing: border-box;
}

.red {
  height: 82rpx;
  line-height: 82rpx;
  font-size: 30rpx;
  color: #FF4B00;
  border-bottom: 6rpx solid #FF4B00;
  box-sizing: border-box;
}

.delivery {
  padding: 106rpx 20rpx 94rpx;
  width: 100%;
  box-sizing: border-box;
}

.deliveryDetails {
  margin-bottom: 24rpx;
  padding: 26rpx 46rpx 8rpx 18rpx;
  width: 100%;
  border-radius: 10rpx;
  background-color: #FFFFFF;
  box-shadow: 0 4rpx 16rpx 0 rgba(0, 0, 0, .3);
  box-sizing: border-box;
}

.deliveryTop {
  display: flex;
}

.deliveryImg {
  margin-right: 32rpx;
}

.deliveryImg image {
  width: 186rpx;
}

.deliveryContent {
  width: 0;
  flex: 1;
}

.deliveryContent view {
  width: 100%;
  height: 44rpx;
  line-height: 44rpx;
  font-size: 26rpx;
  color: #1E1E1E;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.deliveryNum {
  height: 76rpx;
  line-height: 76rpx;
  font-size: 28rpx;
  color: #1E1E1E;
  text-align: right;
}

.signFor {
  height: 30rpx;
  line-height: 30rpx;
  font-size: 20rpx;
  color: #FF5202;
  text-align: right;
}

.nodata {
  padding: 50rpx 0;
  text-align: center;
  font-size: 28rpx;
  color: #818181;
  background-color: #FFFFFF;
  border-radius: 10rpx;
}