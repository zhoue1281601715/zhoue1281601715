.tabbar {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 750rpx;
  height: 94rpx;
  display: flex;
  align-items: center;
  font-size: 0;
  background-color: #FFFFFF;
  z-index: 999;
}

.tabbar-item {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.tabbar-item-icon image {
  width: 42rpx;
  height: 42rpx;
}

.tabbar-item-text {
  font-size: 22rpx;
  color: #8B8B8B;
}

.tabbar-item:nth-child(2) {
  flex: 3;
  position: relative;
  top: -30rpx;
}

.tabbar-item:nth-child(2) .tabbar-item-icon {
  position: relative;
  width: 102rpx;
  height: 102rpx;
  background-color: #FFFFFF;
  border-radius: 50%;
}

.tabbar-item:nth-child(2) .tabbar-item-icon image {
  display: block;
  width: 102rpx!important;
  height: 102rpx!important;
}

.active {
  color: #FF4B00;
}