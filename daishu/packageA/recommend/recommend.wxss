.tip {
  padding: 0 24rpx;
  width: 750rpx;
  line-height: 76rpx;
  font-size: 24rpx;
  color: #6B6969;
  box-sizing: border-box;
}

.container {
  padding: 0 24rpx;
  width: 750rpx;
  box-sizing: border-box
}

.form-content {
  width: 100%;
  border-radius: 10rpx;
  background-color: #FFFFFF;
}

.item {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 24rpx;
  border-bottom: 2rpx solid #E5E5E5;
  box-sizing: border-box;
}

.item:last-child {
  border: none;
}

.item-label {
  width: 40%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  color: #000000;
}

.must {
  display: inline-block;
  margin-right: 10rpx;
  height: 100rpx;
  line-height: 100rpx;
  color: #FF0000;
}

.item-input {
  width: 60%;
  height: 100rpx;
  font-size: 28rpx;
  color: #000000;
  text-align: right;
}

.radio-area {
  display: flex;
  flex-wrap: wrap;
  padding: 12rpx 0;
  width: 60%;
}

.radio-item {
  width: 50%;
  display: flex;
  align-items: center;
  height: 70rpx;
}

.radio-full-width {
  width: 100%;
}

.radio-icon {
  position: relative;
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  border: 2rpx solid #DCDCDC;
  background-color: #FFFFFF;
}

.radio-icon::before {
  display: block;
  content: "\e645";
  padding-left: 2rpx;
  width: 38rpx;
  height: 38rpx;
  line-height: 30rpx;
  font-size: 24rpx;
  color: #FFFFFF;
  font-family: "cuIcon";
  pointer-events: none;
  box-sizing: border-box;
}

.radio-activity {
  border-color: #FF7400;
  background-color: #FF7400;
}

.radio-name {
  margin-left: 24rpx;
  font-size: 28rpx;
  color: #000000;
}

picker {
  width: 60%;
}

.picker {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  text-align: right;
}

.setting {
  padding: 0 30rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  height: 100rpx;
  box-sizing: border-box;
}

.setting-checkbox {
  position: relative;
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  border: 2rpx solid #DCDCDC;
  background-color: #FFFFFF;
}

.setting-checkbox::before {
  display: block;
  content: "\e645";
  padding-left: 2rpx;
  width: 38rpx;
  height: 38rpx;
  line-height: 28rpx;
  font-size: 24rpx;
  color: #FFFFFF;
  font-family: "cuIcon";
  pointer-events: none;
  box-sizing: border-box;
}

.setting-checked {
  border-color: #FF7400;
  background-color: #FF7400;
}

.setting-tip {
  display: flex;
  align-items: center;
  margin-left: 16rpx;
  font-size: 24rpx;
  color: #6B6969;
}

.agreement {
  color: #FF5201;
}

.recommend-btn {
  margin: 0;
  padding: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
  color: #6B6969;
  border-radius: 50rpx;
  background: linear-gradient(90deg, #E5E5E5, #DCDCDC);
}

.recommend-activity {
  color: #FFFFFF;
  background: linear-gradient(90deg, #FF7900, #FF4B00);
}

.reminder {
  margin-top: 30rpx;
  padding: 30rpx 24rpx;
  width: 750rpx;
}

.reminder-title {
  margin-bottom: 20rpx;
  font-size: 24rpx;
  color: #6B6969;
}

.reminder-item {
  line-height: 42rpx;
  font-size: 24rpx;
  color: #6B6969;
}